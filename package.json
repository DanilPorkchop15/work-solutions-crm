  "scripts": {
    "serve": "concurrently \"npm:serve:*\"",
    "serve:watcher": "chokidar \"./package.json\" --debounce 2000 -c \"npm install\"",
    "serve:listen": "http-server -p 2000 --silent",
    "wait": "while ! [ -x /app/node_modules/.bin/wait-port ] ; do sleep 5; echo \"Waiting for wait-root to be installed, that is expected if you are running the container for the first time. Might take some 2-3 minutes.\"; done; wait-port root:2000"
  },
